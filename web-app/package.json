{
  "name": "@loginov-rocks/loginov-rocks-web-app",
  "version": "2.4.2",
  "description": "Web App",
  "scripts": {
    "export": "react-static export",
    "build": "react-static build",
    "bundle": "react-static bundle",
    "serve": "serve dist -p 3000 -s",
    "stage": "react-static build --staging",
    "start": "react-static start",
    "clean:coverage": "rimraf coverage",
    "coverage": "npm run clean:coverage && npm run test:coverage",
    "lint": "eslint --ext js,jsx,ts,tsx lambda src static static.config.js",
    "test": "jest",
    "test:coverage": "npm test -- --coverage",
    "test:update": "npm test -- --updateSnapshot",
    "typecheck": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loginov-rocks/loginov-rocks.git"
  },
  "author": "Danila Loginov <danila@loginov.rocks> (https://loginov.rocks)",
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/loginov-rocks/loginov-rocks/issues"
  },
  "homepage": "https://loginov.rocks",
  "dependencies": {
    "@loginov-rocks/loginov-rocks-shared": "file:../shared",
    "@reach/router": "^1.3.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hot-loader": "^4.13.0",
    "react-static": "^7.5.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.11",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^27.4.1",
    "@types/reach__router": "^1.3.7",
    "@types/react-dom": "^16.9.11",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "aws-sdk": "^2.1119.0",
    "axios": "^0.21.1",
    "babel-jest": "^27.5.1",
    "dotenv": "^16.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "mime": "^3.0.0",
    "prop-types": "^15.8.1",
    "react-static-plugin-css-modules": "^7.5.1",
    "react-static-plugin-reach-router": "^7.5.1",
    "react-static-plugin-sitemap": "^7.5.1",
    "react-static-plugin-source-filesystem": "^7.5.1",
    "react-static-plugin-typescript": "^7.5.1",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "typescript": "^4.6.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/lambda/**/*.js",
      "<rootDir>/src/**/*.{ts,tsx}",
      "<rootDir>/static/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
